{
  "id": null,
  "title": "NAS Monitoring Dashboard",
  "tags": ["nas", "storage"],
  "style": "dark",
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "NAS CPU Usage",
      "type": "gauge",
      "targets": [
        {
          "expr": "100 - (avg by (instance) (irate(node_cpu_seconds_total{mode=\"idle\",job=\"nas-node-exporter\"}[5m])) * 100)",
          "legendFormat": "CPU Usage %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 70
              },
              {
                "color": "red",
                "value": 90
              }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "pluginVersion": "8.0.0",
      "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "title": "NAS Memory Usage",
      "type": "gauge",
      "targets": [
        {
          "expr": "(1 - (node_memory_MemAvailable_bytes{job=\"nas-node-exporter\"} / node_memory_MemTotal_bytes{job=\"nas-node-exporter\"})) * 100",
          "legendFormat": "Memory Usage %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 75
              },
              {
                "color": "red",
                "value": 90
              }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "pluginVersion": "8.0.0",
      "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
    },
    {
      "id": 3,
      "title": "NAS Main Storage",
      "type": "gauge",
      "targets": [
        {
          "expr": "(1 - (node_filesystem_avail_bytes{mountpoint=\"/volume1\",job=\"nas-node-exporter\"} / node_filesystem_size_bytes{mountpoint=\"/volume1\",job=\"nas-node-exporter\"})) * 100",
          "legendFormat": "Volume1 Usage %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 70
              },
              {
                "color": "red",
                "value": 85
              }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "pluginVersion": "8.0.0",
      "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
    },
    {
      "id": 4,
      "title": "NAS Temperature",
      "type": "gauge",
      "targets": [
        {
          "expr": "max(node_hwmon_temp_celsius{job=\"nas-node-exporter\",chip=~\".*coretemp.*\"})",
          "legendFormat": "Max Temp"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "celsius",
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "yellow",
                "value": 60
              },
              {
                "color": "red",
                "value": 80
              }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "pluginVersion": "8.0.0",
      "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
    },
    {
      "id": 5,
      "title": "Storage Usage Details",
      "type": "stat",
      "targets": [
        {
          "expr": "node_filesystem_size_bytes{mountpoint=\"/volume1\",job=\"nas-node-exporter\"} / 1024^4",
          "legendFormat": "Total TB"
        },
        {
          "expr": "node_filesystem_avail_bytes{mountpoint=\"/volume1\",job=\"nas-node-exporter\"} / 1024^4",
          "legendFormat": "Available TB"
        },
        {
          "expr": "(node_filesystem_size_bytes{mountpoint=\"/volume1\",job=\"nas-node-exporter\"} - node_filesystem_avail_bytes{mountpoint=\"/volume1\",job=\"nas-node-exporter\"}) / 1024^4",
          "legendFormat": "Used TB"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 2,
          "custom": {
            "displayMode": "basic"
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto"
      },
      "pluginVersion": "8.0.0",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
    },
    {
      "id": 6,
      "title": "NAS Uptime",
      "type": "stat",
      "targets": [
        {
          "expr": "(node_time_seconds{job=\"nas-node-exporter\"} - node_boot_time_seconds{job=\"nas-node-exporter\"}) / 86400",
          "legendFormat": "Uptime Days"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "d",
          "decimals": 1,
          "custom": {
            "displayMode": "basic"
          }
        }
      },
      "options": {
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "orientation": "auto",
        "textMode": "auto",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "auto"
      },
      "pluginVersion": "8.0.0",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
    },
    {
      "id": 7,
      "title": "NAS CPU Usage Timeline",
      "type": "timeseries",
      "targets": [
        {
          "expr": "100 - (avg by (instance) (irate(node_cpu_seconds_total{mode=\"idle\",job=\"nas-node-exporter\"}[5m])) * 100)",
          "legendFormat": "CPU Usage %"
        },
        {
          "expr": "avg by (instance) (irate(node_cpu_seconds_total{mode=\"system\",job=\"nas-node-exporter\"}[5m])) * 100",
          "legendFormat": "System %"
        },
        {
          "expr": "avg by (instance) (irate(node_cpu_seconds_total{mode=\"user\",job=\"nas-node-exporter\"}[5m])) * 100",
          "legendFormat": "User %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 2,
            "fillOpacity": 10,
            "gradientMode": "none",
            "spanNulls": false,
            "insertNulls": false,
            "showPoints": "never",
            "pointSize": 5,
            "stacking": {
              "mode": "none",
              "group": "A"
            },
            "axisPlacement": "auto",
            "axisLabel": "",
            "scaleDistribution": {
              "type": "linear"
            },
            "hideFrom": {
              "legend": false,
              "tooltip": false,
              "vis": false
            },
            "thresholdsStyle": {
              "mode": "off"
            }
          }
        }
      },
      "options": {
        "tooltip": {
          "mode": "single",
          "sort": "none"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": ["lastNotNull", "max", "mean"]
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
    },
    {
      "id": 8,
      "title": "NAS Memory Usage Timeline",
