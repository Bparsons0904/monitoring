            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "displayMode": "table",
            "placement": "bottom",
            "calcs": ["lastNotNull", "max", "mean"]
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
